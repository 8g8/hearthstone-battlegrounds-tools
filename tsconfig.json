{
  "extends": "@chenyueban/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@main/*": ["src/main/*"],
      "@renderer/*": ["src/renderer/*"],
      "@shared/*": ["src/shared/*"],
      "@core/*": ["src/renderer/core/*"],
      "@logHandler/*": ["src/renderer/logHandler/*"],
      "@suspension/*": ["src/renderer/suspension/*"]
    }
  },
  "exclude": ["test", "release", "src/dist", ".erb/dll"]
}
